         pages = "89-94",
         textVersion =
         paste("Leo Lahti et al.",
               "Dependency detection with similarity constraints",
               "In T{\"u}lay Adali, Jocelyn Chanussot, Christian Jutten, and Jan Larsen, editors.",      
	       "Proc. IEEE International Workshop on Machine Learning for Signal Processing XIX / MLSP09.", 
	       "Piscataway, NJ, USA, pp. 89-94, 2009.",
               sep=" ")
         )
	 
