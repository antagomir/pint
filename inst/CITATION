citHeader("To cite pint in publications use:")


citEntry(entry="inproceedings",
         title = "Dependency detection with similarity constraints",
         author = personList(
	 	person(first ="Leo", last="Lahti", email = "leo.lahti@iki.fi"),
	 	person(first ="Samuel", last="Myllykangas"),
	 	person(first ="Sakari", last="Knuutila"),
	 	person(first ="Samuel", last="Kaski")
	 	  ),
	 note = "In Proc. MLSP'09 IEEE International Workshop on Machine Learning for Signal Processing",
	 year = "2009",
	 publisher = "IEEE",
         textVersion =
         paste("Leo Lahti et al.",
               "Dependency detection with similarity constraints",
               "In Proc. IEEE International Workshop on Machine Learning for Signal Processing (MLSP'09).",
               sep=" ")
         )

citFooter("pint has been created by Olli-Pekka Huovilainen and Leo
          Lahti. Please cite also the pint BioConductor package when
          using it for data analysis.")

